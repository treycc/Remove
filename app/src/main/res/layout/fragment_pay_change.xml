<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:fillViewport="true">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <LinearLayout
            style="@style/Content_LL_T6">

            <TextView
                style="@style/Title_Sub"
                android:text="房屋重置补偿价"/>

            <com.jdp.hls.view.StringTextView
                android:text="60"
                style="@style/Content_ET_W1"
                android:hint="60"/>
        </LinearLayout>

        <View style="@style/div_hor_gray_margin24"/>
        <LinearLayout
            style="@style/Content_LL_T6">

            <TextView
                style="@style/Title_Sub"
                android:text="装修"/>

            <com.jdp.hls.view.StringTextView
                android:text="60"
                style="@style/Content_ET_W1"
                android:hint="60"/>
        </LinearLayout>

        <View style="@style/div_hor_gray_margin24"/>
        <LinearLayout
            style="@style/Content_LL_T6">

            <TextView
                style="@style/Title_Sub"
                android:text="附属物"/>

            <com.jdp.hls.view.StringTextView
                android:text="60"
                style="@style/Content_ET_W1"
                android:hint="60"/>
        </LinearLayout>

        <View style="@style/div_hor_gray_margin24"/>

        <LinearLayout
            style="@style/Content_LL_T6">

            <TextView
                style="@style/Title_Sub"
                android:text="拆迁奖励"/>

            <com.jdp.hls.view.EnableEditText
                android:id="@+id/et_protocol_hzArea"
                style="@style/Content_ET_W1"
                android:hint="请输入户增面积"
                android:text="1000"
                android:inputType="numberDecimal"/>
        </LinearLayout>

        <View style="@style/div_hor_gray_margin24"/>
        <LinearLayout
            style="@style/Content_LL_T6">

            <TextView
                style="@style/Title_Sub"
                android:text="清障奖励"/>

            <com.jdp.hls.view.EnableEditText
                style="@style/Content_ET_W1"
                android:hint="请输入户增面积"
                android:text="1000"
                android:inputType="numberDecimal"/>
        </LinearLayout>

        <View style="@style/div_hor_gray_margin24"/>

        <LinearLayout
            style="@style/Content_LL_T6">

            <TextView
                style="@style/Title_Sub"
                android:text="其它补偿"/>

            <com.jdp.hls.view.EnableEditText
                android:id="@+id/et_protocol_azBuildingArea"
                style="@style/Content_ET_W1"
                android:hint="请输入总安置建筑面积"
                android:text="3500"
                android:inputType="numberDecimal"/>
        </LinearLayout>

        <View style="@style/div_hor_gray_margin24"/>
        <LinearLayout
            style="@style/Content_LL_T6">

            <TextView
                style="@style/Title_Sub"
                android:text="旧房补偿总金额"/>

            <com.jdp.hls.view.StringTextView
                style="@style/Content_ET_W1"
                android:text="60000"
                android:hint="60"/>
        </LinearLayout>

        <View style="@style/div_hor_gray_margin24"/>
        <LinearLayout
            style="@style/Content_LL_T6">

            <TextView
                style="@style/Title_Sub"
                android:text="总购房金额"/>

            <com.jdp.hls.view.EnableEditText
                android:text="50000"
                android:id="@+id/et_protocol_azntArea"
                style="@style/Content_ET_W1"
                android:hint="请输入总安置套内面积"
                android:inputType="numberDecimal"/>
        </LinearLayout>

        <View style="@style/div_hor_gray_margin24"/>
        <LinearLayout
            style="@style/Content_LL_T6">

            <TextView
                style="@style/Title_Sub"
                android:text="应交购房款"/>

            <com.jdp.hls.view.StringTextView
                style="@style/Content_ET_W1"
                android:text="11000"
                android:hint="120000"/>
        </LinearLayout>

        <View style="@style/div_hor_gray_margin24"/>
        <LinearLayout
            style="@style/Content_LL_T6">

            <TextView
                style="@style/Title_Sub"
                android:text="临时安置费"/>

            <com.jdp.hls.view.EnableEditText
                style="@style/Content_ET_W1"
                android:text="1000"
                android:hint="请输入货币补偿总金额"
                android:inputType="numberDecimal"/>
        </LinearLayout>

        <View style="@style/div_hor_gray_margin24"/>
        <LinearLayout
            style="@style/Content_LL_T6">

            <TextView
                style="@style/Title_Sub"
                android:text="搬迁费"/>

            <com.jdp.hls.view.EnableEditText
                style="@style/Content_ET_W1"
                android:text="2000"
                android:hint="请输入货币补偿总金额"
                android:inputType="numberDecimal"/>
        </LinearLayout>

        <View style="@style/div_hor_gray_margin24"/>

        <LinearLayout
            style="@style/Content_LL_T6">

            <TextView
                style="@style/Title_Sub"
                android:text="固定设施报销费"/>

            <com.jdp.hls.view.EnableEditText
                style="@style/Content_ET_W1"
                android:text="3000"
                android:hint="请输入货币补偿总金额"
                android:inputType="numberDecimal"/>
        </LinearLayout>

        <View style="@style/div_hor_gray_margin24"/>

        <LinearLayout
            style="@style/Content_LL_T6">

            <TextView
                style="@style/Title_Sub"
                android:text="总安置建筑面积"/>

            <com.jdp.hls.view.EnableEditText
                android:text="200"
                style="@style/Content_ET_W1"
                android:hint="请输入货币补偿总金额"
                android:inputType="numberDecimal"/>
        </LinearLayout>

        <View style="@style/div_hor_gray_margin24"/>
        <LinearLayout
            style="@style/Content_LL_T6">

            <TextView
                style="@style/Title_Sub"
                android:text="总安置套内面积"/>

            <com.jdp.hls.view.EnableEditText
                android:text="150"
                style="@style/Content_ET_W1"
                android:hint="请输入货币补偿总金额"
                android:inputType="numberDecimal"/>
        </LinearLayout>

        <View style="@style/div_hor_gray_margin24"/>
        <LinearLayout
            style="@style/Content_LL_T6">

            <TextView
                style="@style/Title_Sub"
                android:text="套型"/>

            <com.jdp.hls.view.EnableEditText
                style="@style/Content_ET_W1"
                android:hint=""
                android:text="两室一厅"
                android:inputType="numberDecimal"/>
        </LinearLayout>

        <View style="@style/div_hor_gray_margin24"/>

        <LinearLayout
            style="@style/Content_LL_T6">

            <TextView
                style="@style/Title_Sub"
                android:text="货币补偿总金额"/>

            <com.jdp.hls.view.EnableEditText
                style="@style/Content_ET_W1"
                android:hint=""
                android:text="50000"
                android:inputType="numberDecimal"/>
        </LinearLayout>

        <View style="@style/div_hor_gray_margin24"/>
        <LinearLayout
            style="@style/Content_LL_T6">

            <TextView
                style="@style/Title_Sub"
                android:text="户增面积"/>

            <com.jdp.hls.view.EnableEditText
                style="@style/Content_ET_W1"
                android:hint=""
                android:text="200"
                android:inputType="numberDecimal"/>
        </LinearLayout>

        <View style="@style/div_hor_gray_margin24"/>

        <LinearLayout
            style="@style/Content_LL_T6">

            <TextView
                style="@style/Title_Sub"
                android:text="空翻面积"/>

            <com.jdp.hls.view.EnableEditText
                style="@style/Content_ET_W1"
                android:hint=""
                android:text="150"
                android:inputType="numberDecimal"/>
        </LinearLayout>

        <View style="@style/div_hor_gray_margin24"/>
        <LinearLayout
            style="@style/Content_LL_T6">

            <TextView
                style="@style/Title_Sub"
                android:text="超审批占地"/>

            <com.jdp.hls.view.EnableEditText
                style="@style/Content_ET_W1"
                android:hint=""
                android:text="150"
                android:inputType="numberDecimal"/>
        </LinearLayout>



    </LinearLayout>
</ScrollView>